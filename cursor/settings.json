{
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[javascript][typescript][typescriptreact][json][jsonc][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt"
  },
  "[xml][github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
	"prettier.semi": false,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
  "crashReporting.enabled": "off",
  "cursor.composer.queueMessageDefaultBehavior": "stop-and-send",
  "cursor.composer.usageSummaryDisplay": "never",
  "cursor.cpp.disabledLanguages": [],
  "cursor.cpp.enablePartialAccepts": true,
  "cursor.preferNotificationsSameAsChat": true,
  "debug.console.wordWrap": false,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.cursorStyle": "block",
  "editor.fontFamily": "Jetbrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnPaste": false,
  "editor.guides.bracketPairs": "active",
  "editor.hover.delay": 1000,
  "editor.hover.hidingDelay": 50,
  "editor.hover.sticky": true,
  "editor.inlineSuggest.enabled": false,
  "editor.lineHeight": 0,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "selection",
  "editor.scrollBeyondLastLine": false,
  "editor.stickyScroll.enabled": true,
  "eslint.enable": true,
  "eslint.format.enable": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.decorations.badges": true,
  "explorer.decorations.colors": true,
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "type",
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "git.blame.editorDecoration.enabled": false,
  "git.blame.statusBarItem.enabled": true,
  "prettier.arrowParens": "avoid",
  "redhat.telemetry.enabled": true,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.feedback.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "window.customTitleBarVisibility": "never",
  "window.dialogStyle": "custom",
  "window.nativeTabs": true,
  "window.restoreWindows": "none",
  "window.zoomLevel": 1.25,
  "workbench.activityBar.location": "top",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.tabActionLocation": "right",
  "workbench.editor.wrapTabs": false,
  "workbench.fontAliasing": "auto",
  "workbench.hover.delay": 1000,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Cursor Dark",
  "workbench.preferredLightColorTheme": "Cursor Light",
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 4,
  "workbench.tree.renderIndentGuides": "none",
  "yaml.maxItemsComputed": 10000,
}
