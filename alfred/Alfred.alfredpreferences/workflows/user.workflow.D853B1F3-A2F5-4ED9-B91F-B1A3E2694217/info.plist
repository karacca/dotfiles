				<string>function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

function run(argv) {
  const title = `&lt;h1&gt;${envVar("note_title")}&lt;/h1&gt;&lt;br&gt;`
  const body = argv[0].replaceAll("\n", "&lt;br&gt;")
  const notesApp = Application("Notes")
  const folder = notesApp.folders.byName(envVar("new_notes_folder"))
  const newNote = notesApp.Note({ body: title + body })

  if (!folder.exists()) notesApp.folders.push(notesApp.Folder({ name: envVar("new_notes_folder") }))

  folder.notes.push(newNote)
}</string>
